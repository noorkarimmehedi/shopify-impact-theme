{% doc %}
  @prompt
    create a collection section where in mobile version  3 products should be visible one in the middle and the rest two on left and right side. the middle one slightly big the rest two same size . also use arrow below that to navigate between products. and also i want clickable neobrutalism ui. from color to shadow to font to radius all should be customizable
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-neo-collection-{{ ai_gen_id }} {
    padding: 40px 20px;
    background-color: {{ block.settings.section_background }};
  }

  .ai-neo-collection__header-{{ ai_gen_id }} {
    text-align: center;
    margin-bottom: 40px;
  }

  .ai-neo-collection__title-{{ ai_gen_id }} {
    font-size: {{ block.settings.title_size }}px;
    font-weight: {{ block.settings.title_weight }};
    color: {{ block.settings.title_color }};
    margin: 0 0 12px;
    text-transform: {{ block.settings.title_transform }};
    letter-spacing: {{ block.settings.title_spacing }}px;
  }

  .ai-neo-collection__subtitle-{{ ai_gen_id }} {
    font-size: {{ block.settings.subtitle_size }}px;
    color: {{ block.settings.subtitle_color }};
    margin: 0;
  }

  .ai-neo-collection__carousel-{{ ai_gen_id }} {
    position: relative;
    max-width: 100%;
    margin: 0 auto;
  }

  .ai-neo-collection__track-{{ ai_gen_id }} {
    display: flex;
    gap: {{ block.settings.product_gap }}px;
    overflow-x: auto;
    scroll-snap-type: x mandatory;
    scroll-behavior: smooth;
    scrollbar-width: none;
    -ms-overflow-style: none;
    padding: 20px 0;
  }

  .ai-neo-collection__track-{{ ai_gen_id }}::-webkit-scrollbar {
    display: none;
  }

  .ai-neo-collection__product-{{ ai_gen_id }} {
    flex-shrink: 0;
    scroll-snap-align: center;
    transition: transform 0.3s ease;
  }

  .ai-neo-collection__product-{{ ai_gen_id }}--side {
    width: calc((100% - {{ block.settings.product_gap }}px * 2) * 0.28);
  }

  .ai-neo-collection__product-{{ ai_gen_id }}--center {
    width: calc((100% - {{ block.settings.product_gap }}px * 2) * 0.44);
  }

  .ai-neo-collection__card-{{ ai_gen_id }} {
    background-color: {{ block.settings.card_background }};
    border: {{ block.settings.border_width }}px solid {{ block.settings.border_color }};
    border-radius: {{ block.settings.border_radius }}px;
    box-shadow: {{ block.settings.shadow_x }}px {{ block.settings.shadow_y }}px 0px {{ block.settings.shadow_color }};
    overflow: hidden;
    transition: all 0.2s ease;
    height: 100%;
    display: flex;
    flex-direction: column;
  }

  .ai-neo-collection__card-{{ ai_gen_id }}:hover {
    transform: translate({{ block.settings.hover_translate_x }}px, {{ block.settings.hover_translate_y }}px);
    box-shadow: {{ block.settings.hover_shadow_x }}px {{ block.settings.hover_shadow_y }}px 0px {{ block.settings.shadow_color }};
  }

  .ai-neo-collection__image-wrapper-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    padding-bottom: 100%;
    overflow: hidden;
    background-color: {{ block.settings.image_background }};
  }

  .ai-neo-collection__image-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .ai-neo-collection__info-{{ ai_gen_id }} {
    padding: {{ block.settings.card_padding }}px;
    flex-grow: 1;
    display: flex;
    flex-direction: column;
  }

  .ai-neo-collection__product-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.product_title_size }}px;
    font-weight: {{ block.settings.product_title_weight }};
    color: {{ block.settings.product_title_color }};
    margin: 0 0 8px;
    line-height: 1.3;
  }

  .ai-neo-collection__price-{{ ai_gen_id }} {
    font-size: {{ block.settings.price_size }}px;
    font-weight: {{ block.settings.price_weight }};
    color: {{ block.settings.price_color }};
    margin-bottom: 12px;
  }

  .ai-neo-collection__button-{{ ai_gen_id }} {
    display: inline-block;
    width: 100%;
    padding: {{ block.settings.button_padding_y }}px {{ block.settings.button_padding_x }}px;
    background-color: {{ block.settings.button_background }};
    color: {{ block.settings.button_text_color }};
    border: {{ block.settings.button_border_width }}px solid {{ block.settings.button_border_color }};
    border-radius: {{ block.settings.button_radius }}px;
    box-shadow: {{ block.settings.button_shadow_x }}px {{ block.settings.button_shadow_y }}px 0px {{ block.settings.button_shadow_color }};
    font-size: {{ block.settings.button_text_size }}px;
    font-weight: {{ block.settings.button_text_weight }};
    text-align: center;
    text-decoration: none;
    cursor: pointer;
    transition: all 0.2s ease;
    text-transform: {{ block.settings.button_text_transform }};
    letter-spacing: {{ block.settings.button_letter_spacing }}px;
    margin-top: auto;
  }

  .ai-neo-collection__button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.button_hover_background }};
    color: {{ block.settings.button_hover_text_color }};
    transform: translate({{ block.settings.button_hover_translate_x }}px, {{ block.settings.button_hover_translate_y }}px);
    box-shadow: {{ block.settings.button_hover_shadow_x }}px {{ block.settings.button_hover_shadow_y }}px 0px {{ block.settings.button_shadow_color }};
  }

  .ai-neo-collection__controls-{{ ai_gen_id }} {
    display: flex;
    justify-content: center;
    gap: {{ block.settings.arrow_gap }}px;
    margin-top: 30px;
  }

  .ai-neo-collection__arrow-{{ ai_gen_id }} {
    width: {{ block.settings.arrow_size }}px;
    height: {{ block.settings.arrow_size }}px;
    background-color: {{ block.settings.arrow_background }};
    border: {{ block.settings.arrow_border_width }}px solid {{ block.settings.arrow_border_color }};
    border-radius: {{ block.settings.arrow_radius }}px;
    box-shadow: {{ block.settings.arrow_shadow_x }}px {{ block.settings.arrow_shadow_y }}px 0px {{ block.settings.arrow_shadow_color }};
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
    transition: all 0.2s ease;
    padding: 0;
  }

  .ai-neo-collection__arrow-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.arrow_hover_background }};
    transform: translate({{ block.settings.arrow_hover_translate_x }}px, {{ block.settings.arrow_hover_translate_y }}px);
    box-shadow: {{ block.settings.arrow_hover_shadow_x }}px {{ block.settings.arrow_hover_shadow_y }}px 0px {{ block.settings.arrow_shadow_color }};
  }

  .ai-neo-collection__arrow-{{ ai_gen_id }}:disabled {
    opacity: 0.3;
    cursor: not-allowed;
  }

  .ai-neo-collection__arrow-icon-{{ ai_gen_id }} {
    width: 50%;
    height: 50%;
    stroke: {{ block.settings.arrow_icon_color }};
    stroke-width: {{ block.settings.arrow_icon_weight }};
  }

  .ai-neo-collection__empty-{{ ai_gen_id }} {
    text-align: center;
    padding: 60px 20px;
    color: {{ block.settings.subtitle_color }};
  }

  @media screen and (min-width: 750px) {
    .ai-neo-collection__track-{{ ai_gen_id }} {
      justify-content: center;
    }

    .ai-neo-collection__product-{{ ai_gen_id }}--side {
      width: 280px;
    }

    .ai-neo-collection__product-{{ ai_gen_id }}--center {
      width: 360px;
    }
  }
{% endstyle %}

<neo-collection-{{ ai_gen_id }} class="ai-neo-collection-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  {% if block.settings.title != blank or block.settings.subtitle != blank %}
    <div class="ai-neo-collection__header-{{ ai_gen_id }}">
      {% if block.settings.title != blank %}
        <h2 class="ai-neo-collection__title-{{ ai_gen_id }}">{{ block.settings.title }}</h2>
      {% endif %}
      {% if block.settings.subtitle != blank %}
        <p class="ai-neo-collection__subtitle-{{ ai_gen_id }}">{{ block.settings.subtitle }}</p>
      {% endif %}
    </div>
  {% endif %}

  {% if block.settings.collection != blank %}
    <div class="ai-neo-collection__carousel-{{ ai_gen_id }}">
      <div class="ai-neo-collection__track-{{ ai_gen_id }}" data-track>
        {% for product in block.settings.collection.products limit: block.settings.product_limit %}
          {% assign is_center = false %}
          {% assign remainder = forloop.index | modulo: 3 %}
          {% if remainder == 2 %}
            {% assign is_center = true %}
          {% endif %}

          <div class="ai-neo-collection__product-{{ ai_gen_id }} {% if is_center %}ai-neo-collection__product-{{ ai_gen_id }}--center{% else %}ai-neo-collection__product-{{ ai_gen_id }}--side{% endif %}">
            <a href="{{ product.url }}" class="ai-neo-collection__card-{{ ai_gen_id }}">
              <div class="ai-neo-collection__image-wrapper-{{ ai_gen_id }}">
                {% if product.featured_image %}
                  <img
                    src="{{ product.featured_image | image_url: width: 800 }}"
                    alt="{{ product.featured_image.alt | escape }}"
                    class="ai-neo-collection__image-{{ ai_gen_id }}"
                    loading="lazy"
                    width="800"
                    height="800"
                  >
                {% else %}
                  <div class="ai-neo-collection__image-placeholder-{{ ai_gen_id }}">
                    {{ 'product-1' | placeholder_svg_tag }}
                  </div>
                {% endif %}
              </div>
              <div class="ai-neo-collection__info-{{ ai_gen_id }}">
                <h3 class="ai-neo-collection__product-title-{{ ai_gen_id }}">{{ product.title }}</h3>
                <div class="ai-neo-collection__price-{{ ai_gen_id }}">
                  {{ product.price | money }}
                </div>
                <span class="ai-neo-collection__button-{{ ai_gen_id }}">
                  {{ block.settings.button_text }}
                </span>
              </div>
            </a>
          </div>
        {% endfor %}
      </div>

      <div class="ai-neo-collection__controls-{{ ai_gen_id }}">
        <button
          class="ai-neo-collection__arrow-{{ ai_gen_id }} ai-neo-collection__arrow-prev-{{ ai_gen_id }}"
          aria-label="Previous products"
          data-prev
        >
          <svg class="ai-neo-collection__arrow-icon-{{ ai_gen_id }}" viewBox="0 0 24 24" fill="none" stroke-linecap="round" stroke-linejoin="round">
            <polyline points="15 18 9 12 15 6"></polyline>
          </svg>
        </button>
        <button
          class="ai-neo-collection__arrow-{{ ai_gen_id }} ai-neo-collection__arrow-next-{{ ai_gen_id }}"
          aria-label="Next products"
          data-next
        >
          <svg class="ai-neo-collection__arrow-icon-{{ ai_gen_id }}" viewBox="0 0 24 24" fill="none" stroke-linecap="round" stroke-linejoin="round">
            <polyline points="9 18 15 12 9 6"></polyline>
          </svg>
        </button>
      </div>
    </div>
  {% else %}
    <div class="ai-neo-collection__empty-{{ ai_gen_id }}">
      <p>Select a collection to display products</p>
    </div>
  {% endif %}
</neo-collection-{{ ai_gen_id }}>

<script>
  (function() {
    class NeoCollection{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.track = this.querySelector('[data-track]');
        this.prevButton = this.querySelector('[data-prev]');
        this.nextButton = this.querySelector('[data-next]');
        this.currentIndex = 0;
      }

      connectedCallback() {
        if (!this.track) return;

        this.products = Array.from(this.track.children);
        this.totalProducts = this.products.length;

        if (this.totalProducts === 0) return;

        this.prevButton.addEventListener('click', () => this.navigate(-1));
        this.nextButton.addEventListener('click', () => this.navigate(1));

        this.track.addEventListener('scroll', () => this.updateButtons());

        this.updateButtons();
      }

      navigate(direction) {
        const productWidth = this.products[0].offsetWidth;
        const gap = parseInt(getComputedStyle(this.track).gap) || 0;
        const scrollAmount = (productWidth + gap) * 3;

        this.track.scrollBy({
          left: direction * scrollAmount,
          behavior: 'smooth'
        });
      }

      updateButtons() {
        const scrollLeft = this.track.scrollLeft;
        const maxScroll = this.track.scrollWidth - this.track.clientWidth;

        this.prevButton.disabled = scrollLeft <= 10;
        this.nextButton.disabled = scrollLeft >= maxScroll - 10;
      }
    }

    customElements.define('neo-collection-{{ ai_gen_id }}', NeoCollection{{ ai_gen_id }});
  })();
</script>

